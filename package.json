{
	"name": "geshido",
	"version": "0.1.0",
	"description": "Recurring task tracker",
	"main": "public/index.js",
	"scripts": {
		"build": "npm run check && parcel build ./src/index.html --out-dir ./dist/public",
		"check": "npm run lint && npm run typecheck",
		"clean": "rimraf ./public",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
		"serve": "concurrently --kill-others \"npm run watch\" \"firebase serve\"",
		"typecheck": "tsc --project ./tsconfig.json --noEmit",
		"watch": "parcel watch ./src/*.html --out-dir ./public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fongandrew/geshido.git"
	},
	"author": "Andrew Fong",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fongandrew/geshido/issues"
	},
	"homepage": "https://github.com/fongandrew/geshido#readme",
	"devDependencies": {
		"@types/lodash": "^4.14.136",
		"@types/react": "^16.9.1",
		"@types/react-dom": "^16.8.5",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"concurrently": "^4.1.1",
		"eslint": "^6.1.0",
		"eslint-config-airbnb-typescript": "^4.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.6.0",
		"firebase-tools": "^7.2.2",
		"parcel-bundler": "^1.12.3",
		"prettier": "^1.17.0",
		"rimraf": "^2.6.3",
		"typescript": "^3.5.3"
	},
	"dependencies": {
		"firebase": "^6.3.5",
		"lodash": "^4.17.15",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"regenerator-runtime": "^0.13.3"
	}
}
